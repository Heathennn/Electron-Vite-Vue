{
  "name": "Gear",
  "version": "0.0.1",
  "main": "dist/electron/main/index.js",
  "description": "Gear",
  "author": "Gnak",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.1",
    "electron": "^20.0.2",
    "electron-builder": "^23.3.3",
    "less": "^4.1.3",
    "pinia": "^2.0.18",
    "typescript": "^4.7.4",
    "vite": "^3.0.5",
    "vite-plugin-electron": "^0.10.2",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vue-tsc": "^0.40.1"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "build": {
    "productName": "Gear",
    "appId": "com.electron.Gear",
    "extraFiles": [
      {
        "from": "./pyScript/",
        "to": "./pyScript/"
      }
    ],
    "files": [
      "dist/**/*"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
